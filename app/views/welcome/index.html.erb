<!-- Header -->
<header class="header header-inverse h-fullscreen py-0" data-overlay="7">
  <div class="header_video">
    <%= video_tag "coming_soon.mp4", controls: false, autoplay: true, muted: true, loop: true, autobuffer: true, class: "fullscreen-bg-video video", type: 'video/mp4' %>
  </div>
  <div class="container text-center">

    <div class="row h-full align-items-center">
      <div class="col-12 col-lg-8 offset-lg-2">

        <%= image_tag 'logo', class: 'col-lg-5 ml-auto' %>
        <br><br>
        <br><br>
        <br>
        <div class="countdown countdown-sm countdown-outline countdown-inverse countdown-uppercase" data-countdown="2018/09/1"></div>

      </div>
    </div>

  </div>
</header>
<!-- END Header -->
